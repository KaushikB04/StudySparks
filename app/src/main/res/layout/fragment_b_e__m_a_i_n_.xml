<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BE_MAIN_Fragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:background="@drawable/backgroundphoto2"
        >
        <TextView
            android:id="@+id/BranchBECivil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Civil Engineering"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />
        <TextView
            android:id="@+id/BranchBEComputer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Computer Engineering"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />
        <TextView
            android:id="@+id/BranchBEENTC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Electronics And Telecommunication Engineering"
            android:textAlignment="center"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />
        <TextView
            android:id="@+id/BranchBEElectrical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Electrical Engineering"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />
        <TextView
            android:id="@+id/BranchBEIT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Information Technology Engineering"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />
        <TextView
            android:id="@+id/BranchBEMechanical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mechanical Engineering"
            android:layout_marginTop="@dimen/_20sdp"
            android:fontFamily="@font/spacemid"
            android:textSize="@dimen/_15ssp"
            android:textColor="@color/DarkBlue"
            />

    </LinearLayout>


</FrameLayout>